{
	"name": "pixlcore.com",
	"private": true,
	"version": "1.0.0",
	"description": "The PixlCore.com website.",
	"author": "Joseph Huckaby <jhuckaby@pixlcore.com>",
	"homepage": "https://github.com/pixlcore/pixlcore.com",
	"license": "Private",
	"main": "lib/main.js",
	"scripts": {
		"install": "node bin/build.js dist"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/pixlcore/pixlcore.com"
	},
	"bugs": {
		"url": "https://github.com/pixlcore/pixlcore.com/issues"
	},
	"keywords": [
		"pixlcore",
		"webapp"
	],
	"dependencies": {
		"@mdi/font": "7.4.47",
		"@mdi/svg": "7.4.47",
		"async": "2.6.4",
		"class-plus": "^2.0.0",
		"jquery": "3.6.0",
		"marked": "4.3.0",
		"mime": "2.5.2",
		"codemirror": "5.58.3",
		"pixl-cache": "^1.0.6",
		"pixl-cli": "^1.0.11",
		"pixl-request": "^2.0.3",
		"pixl-tools": "^1.0.24",
		"pixl-xml": "^1.0.13",
		"pixl-xyapp": "^2.1.0",
		"uglify-js": "3.13.5"
	},
	
	"pool": "default",
	"acl": false,
	"headers": {
		"host": "^(www\\.|local\\.)?(pixlcore\\.com|joemax)(\\:\\d+)?$"
	},
	"routes": [
		{
			"type": "static",
			"uri_match": "^/\\.well\\-known",
			"path": "/opt/poolnoodle/htdocs/.well-known"
		},
		{
			"type": "proxy",
			"uri_match": "^/(archives|bugs|canvas-plus|demos|public|screenshots|software|temp|uploads)/",
			"target_hostname": "s3.us-west-1.amazonaws.com",
			"target_port": 80,
			"preserve_host": false,
			"path_prefix": "/pixlcore.com",
			"use_keep_alives": true,
			"dir_index": "index.html",
			"insert_response_headers": {
				"Cache-Control": "public, max-age=86400"
			}
		},
		{
			"type": "script",
			"uri_match": "^/(blog/|view/|doc/|about/|feed\\.rss|index\\.html|$)",
			"path": "lib/api.js",
			"max_requests_per_sec": 1000,
			"max_concurrent_requests": 100
		},
		{
			"type": "static",
			"uri_match": "^/",
			"path": "htdocs"
		}
	]
}
